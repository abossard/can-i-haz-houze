{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.SignalR": "Information",
      "Microsoft.AspNetCore.Http.Connections": "Information"
    }
  },
  "AllowedHosts": "*",
  "CircuitOptions": {
    "DisconnectedCircuitMaxRetained": 100,
    "DisconnectedCircuitRetentionPeriod": "00:03:00",
    "JSInteropDefaultCallTimeout": "00:01:00",
    "MaxBufferedUnacknowledgedRenderBatches": 10
  },
  "Kestrel": {
    "Limits": {
      "MaxConcurrentConnections": 200,
      "MaxConcurrentUpgradedConnections": 200,
      "KeepAliveTimeout": "00:02:00",
      "RequestHeadersTimeout": "00:00:30"
    }
  }
}
