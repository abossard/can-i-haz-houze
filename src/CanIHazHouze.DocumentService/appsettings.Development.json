{  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.Extensions.Hosting": "Information",
      "CanIHazHouze.DocumentService": "Debug"
    }
  },
  "DocumentStorage": {
    "ContainerName": "documents-dev",
    "MaxFileSizeBytes": 52428800
  },
  "MCP": {
    "Enabled": true,
    "Endpoint": "/mcp",
    "MaxConnections": 100,
    "MessageSizeLimit": 1048576,
    "AllowedOrigins": ["*"]
  }
}
